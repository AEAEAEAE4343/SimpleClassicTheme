  <data name="updateString" xml:space="preserve">
    <value>@echo off
title SCT Update
echo Starting installation...
timeout 2 &gt; nul
echo Killing SCT...
taskkill /im %2 /f
echo Removing old files...
del %2
echo Installing SCT v%1...
copy %3 %2
echo Starting SCT...
%2
echo Removing temporary files...
del %3
(goto) 2&gt;nul &amp; del "%~f0"</value>
